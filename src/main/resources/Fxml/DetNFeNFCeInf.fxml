<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<StackPane fx:id="detNFeNFCeInfPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dechivejavafx.gui.DetNFeNFCeInfController">
	<VBox alignment="TOP_LEFT" fillWidth="true" prefHeight="700.0" prefWidth="2200.0" style="-fx-padding: 10px;">
		<Label text="Arquivo em InfNFE e InfNFCe que não está em Det ou NFe ou NFCe.">
			<font>
				<Font name="System Bold" size="14.0" />
			</font>
			<padding>
				<Insets left="5.0" top="5.0" />
			</padding>
		</Label>

		<HBox spacing="10" HBox.hgrow="ALWAYS">
			<!-- Se necessário, adicione conteúdo aqui -->
		</HBox>
		<HBox spacing="10" HBox.hgrow="ALWAYS">
			<TableView fx:id="tableViewDetNFeNFCeInf" VBox.vgrow="ALWAYS" prefHeight="573.0" prefWidth="1185.0">
				<columns>
					<TableColumn fx:id="columnArquivo" text="Arquivo" prefWidth="200">
						<cellValueFactory>
							<PropertyValueFactory property="arquivo" />
						</cellValueFactory>
					</TableColumn>
					<TableColumn fx:id="columnTabelaDetalhe" text="Tabela de Detalhe" prefWidth="200">
						<cellValueFactory>
							<PropertyValueFactory property="tabelaDetalhe" />
						</cellValueFactory>
					</TableColumn>
					<TableColumn fx:id="columnQuantidadeNsuchave" text="Quantidade de nsuchave" prefWidth="200">
						<cellValueFactory>
							<PropertyValueFactory property="quantidadeNsuchave" />
						</cellValueFactory>
					</TableColumn>
				</columns>
				<columnResizePolicy>
					<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
				</columnResizePolicy>
			</TableView>
		</HBox>
	</VBox>
</StackPane>
